<!DOCTYPE html>
<html>
<head>
	<title>freastal technologies</title>

  

    <!-- css -->
    <link rel="stylesheet" type="text/css" href="./css/style.css">
    <link rel="stylesheet" type="text/css" href="./css/style2.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- font -->
    <style>
    @import url('https://fonts.googleapis.com/css?family=Nunito');
    </style> 

    <script src="https://code.jquery.com/jquery-3.3.1.slim.js" integrity="sha256-fNXJFIlca05BIO2Y5zh1xrShK3ME+/lYZ0j+ChxX2DA="crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/js/materialize.min.js"></script>

</head>
<body>
<% include header %>

  <nav>
    <div class="nav-wrapper">
      <ul id="nav-mobile" class="left hide-on-med-and-down">
        <li><a class="waves-effect waves-light btn modal-trigger" href="#modal1">New Client</a></li>
        <li><a class="waves-effect waves-light btn" href="/attendance/absent">Absent Client</a></li>
        <li><a class="waves-effect waves-light btn" href="/attendance/present">Present Client</a></li>
        <li><a class="waves-effect waves-light btn" href="/bulksms">Bulk SMS</a></li>
        <li><a class="input-field right hide-on-med-and-down right">
          <form action="/" method="GET">
          <input id="search" type="search" name="search">
          <a class="label-icon" onkeypress="dosomething();" for="search"><i class="material-icons">search</i></a>
          </form>
        </a></li>
      </ul>
    </div>
  </nav>

  <div id="modal1" class="modal">
    <div class="modal-content">
      <h4>NEW CLIENT</h4>
      <p>A bunch of text</p>
    </div>
    <div class="modal-footer">
      <a href="/newclient" class="modal-close waves-effect waves-green btn-flat">Gym member</a>
      <a href="/newclient" class="modal-close waves-effect waves-green btn-flat">Personal traning</a>
      <a href="/newclient" class="modal-close waves-effect waves-green btn-flat">Group class</a>
    </div>
  </div>

<div class="section9">
  <div class="container">
    <form action="/setting/addpackage" method="POST">
    <div class="row">
        <div class="row">
        <div class="input-field col s12">
             <i class="material-icons prefix">account_circle</i>
          <input id="clientname" type="text" name="clientname" class="validate">
          <label for="clientname">Client Name</label>
        </div>
      </div>
       <div class="row">
        <div class="input-field col s12">
             <i class="material-icons prefix">account_circle</i>
          <input id="packagename" type="text" name="packagename" class="validate">
          <label for="packagename">Package Name</label>
        </div>
      </div>
        <div class="row">
        <div class="input-field col s12">
             <i class="material-icons prefix">account_circle</i>
          <input id="duration" type="text" name="duration" class="validate" value="180 365">
          <label for="duration">Duration(in days)</label>
        </div>
      </div>
       <div class="row">
        <div class="input-field col s12">
             <i class="material-icons prefix">account_circle</i>
          <input id="packagefees" type="text" name="packagefees" class="validate">
          <label for="packagefees">Package Fees</label>
        </div>
      </div>
      <div class="row">
         <div class="input-field col s12">

            <select name="status">
                    <option name="group10" value="active" >Active</option>
                    <option name="group10" value="inactive" >Inactive</option>
            </select>
            <label for="status" name="status"> Status </label>
        </div>
      </div>
    
    <input type="hidden" name="instant" value="0"><div class="input-field col l12 s12 m12 center">
            <button class="btn waves-effect waves-light" id="btnsubmit" type="submit" name="register">submit </button>
    
     </form>
    </div>
  </div>

  <div class="section10">
  <div class="row">
    <div class="col s12 m12 l12">
      <table>
        <thead>
          <tr>
              <th>Client Name</th>
              <th>Package Name</th>
              <th>Duration</th>
              <th>Packagefees</th>
              <th>Status</th>
              <th>Action</th>
          </tr>
        </thead>

        <% package.forEach(function(package){ %>
           <tbody>
          <tr>
            <td><%= package.clientname %></td>
            <td><%= package.packagename %></td>
            <td><%= package.duration %></td>
            <td><%= package.packagefees %></td>
            <td><%= package.status %></td>
            <td><a href="/setting/addpackage/edit/<%= package._id %>">edit</a></td>
          </tr> 
        </tbody>
        <% }); %>
      </table>
      </div>
  </div>
  </div>


<script>
    $(document).ready(function(){
    $('.modal').modal();
  });

  $(document).ready(function(){
    $('.datepicker').datepicker();
  });

    $(document).ready(function(){
    $('.timepicker').timepicker();
  });

    $(document).ready(function(){
    $('select').formSelect();
  });
</script>


<script src="https://code.jquery.com/jquery-3.3.1.slim.js" integrity="sha256-fNXJFIlca05BIO2Y5zh1xrShK3ME+/lYZ0j+ChxX2DA="crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/js/materialize.min.js"></script>


</body>
</html>